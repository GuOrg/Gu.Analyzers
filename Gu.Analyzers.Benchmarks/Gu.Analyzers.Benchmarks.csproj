<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net461</TargetFrameworks>
    <OutputType>Exe</OutputType>
    <CodeAnalysisRuleSet>Gu.Analyzers.Benchmarks.ruleset</CodeAnalysisRuleSet>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <NoWarn>NU1605</NoWarn>
  </PropertyGroup>
  
  <ItemGroup>
    <Content Include="TraceEvent.ReadMe.txt">
      <Paket>True</Paket>
    </Content>
    <Content Include="TraceEvent.ReleaseNotes.txt">
      <Paket>True</Paket>
    </Content>
    <Content Include="_TraceEventProgrammersGuide.docx">
      <Paket>True</Paket>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Gu.Analyzers.Analyzers\Gu.Analyzers.Analyzers.csproj" />
    <ProjectReference Include="..\Gu.Analyzers.CodeFixes\Gu.Analyzers.CodeFixes.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Benchmarks\" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>